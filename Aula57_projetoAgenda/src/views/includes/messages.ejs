<% if (typeof erros !== 'undefined' && erros.length > 0) { %>
<div class="row">
  <div class="alert alert-danger">
    <% erros.forEach(erro => { %> <%= erro %><br />
    <% }) %>
  </div>
</div>
<% } %> <% if (typeof success !== 'undefined' && success.length > 0) { %>
<div class="row">
  <div class="alert alert-success">
    <% success.forEach(sucess => { %> <%= sucess %><br />
    <% }) %>
  </div>
</div>
<% } %>
